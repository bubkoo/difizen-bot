{
  "name": "bot",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub App for difizen group",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint 'src/**/*.{js,ts}?(x)' --fix",
    "build": "tsc --outDir ./lib",
    "build:watch": "run-s 'build -- -w'",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "prebuild": "run-s clean"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "pretty-quick â€”-staged"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "author": {
    "name": "bubkooo",
    "email": "bubkoo.wy@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "gemoji": "6.1.0",
    "js-yaml": "^4.1.0",
    "probot": "^13.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@bubkoo/commitlint-config": "^2.0.0",
    "@bubkoo/eslint-config": "^3.1.1",
    "@bubkoo/husky-hooks": "^2.0.1",
    "@bubkoo/prettier-config": "^2.0.0",
    "@bubkoo/semantic-release-config": "^2.0.0",
    "@bubkoo/tsconfig": "^2.0.0",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^22.9.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.15.0",
    "nock": "^13.3.0",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "smee-client": "^2.0.4",
    "tslib": "^2.5.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">= 20"
  },
  "repository": "https://github.com/difizen/bot.git",
  "homepage": "https://github.com/difizen/bot",
  "bugs": "https://github.com/difizen/bot/issues"
}
